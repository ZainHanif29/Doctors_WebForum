@model IEnumerable<Doctors_WebForum.Models.UserReg>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}
<h1 style="text-align:center">User Register Record</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Password</th>
            <th scope="col">Contact</th>
            <th scope="col">Address</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td scope="row">@Html.DisplayFor(modelItem => item.UId)</td>
                <td>@Html.DisplayFor(modelItem => item.UName)</td>
                <td>@Html.DisplayFor(modelItem => item.UEmail)</td>
                <td>@Html.DisplayFor(modelItem => item.UPassword)</td>
                <td>@Html.DisplayFor(modelItem => item.UContact)</td>
                <td>@Html.DisplayFor(modelItem => item.UAddress)</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.UId">Edit</a>
                </td>
                <td>
                    <a asp-action="Delete" asp-route-id="@item.UId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
