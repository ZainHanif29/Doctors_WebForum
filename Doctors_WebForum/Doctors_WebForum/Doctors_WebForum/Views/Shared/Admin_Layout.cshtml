<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Doctors_WebForum</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Doctors_WebForum.styles.css" asp-append-version="true" />
</head>
<body>
    <nav class="navbar navbar-light bg-light fixed-top">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">Admin Panel</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar" aria-controls="offcanvasNavbar">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="offcanvas offcanvas-end" tabindex="-1" id="offcanvasNavbar" aria-labelledby="offcanvasNavbarLabel">
                <div class="offcanvas-header">
                    <h5 class="offcanvas-title" id="offcanvasNavbarLabel">Close</h5>
                    <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
                </div>
                <div class="offcanvas-body">
                    <ul class="navbar-nav justify-content-end flex-grow-1 pe-3">
                        <li class="nav-item">
                            <a class="nav-link active" asp-area="" asp-controller="DoctorRegsHome" asp-action="Index">Home</a>
                        </li>
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Admin Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="AdminRegs" asp-action="Index">Table List</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="AdminRegs" asp-action="Create">Create New</a></li>
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Doctor Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="DoctorRegs" asp-action="Index">Table List</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="DoctorRegs" asp-action="Create">Create New</a></li>
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                User Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="UserRegs" asp-action="Index">Table List</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="UserRegs" asp-action="Create">Create New</a></li>
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                        <hr /> <hr />
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Qualifications Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="Qualifications" asp-action="Index">Table List</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="Qualifications" asp-action="Create">Create New</a></li>
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Specializations Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="Specializations" asp-action="Index">Table List</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="Specializations" asp-action="Create">Create New</a></li>
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Experiences Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="Experiences" asp-action="Index">Table List</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="Experiences" asp-action="Create">Create New</a></li>
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Privacies Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="Privacies" asp-action="Index">Table List</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="Privacies" asp-action="Create">Create New</a></li>
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                        
                        <hr /> <hr />
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Appointment Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="DoctorAppointmentsadmin" asp-action="Index">Table List</a></li>
                                @*<li><a class="dropdown-item" asp-area="" asp-controller="UserRegs" asp-action="Create">Create New</a></li>*@
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                        @*-----------------------------------------*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="offcanvasNavbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Contact Page
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="offcanvasNavbarDropdown">
                                <li><a class="dropdown-item" asp-area="" asp-controller="Contactsadmin" asp-action="Index">Table List</a></li>
                                @*<li><a class="dropdown-item" asp-area="" asp-controller="UserRegs" asp-action="Create">Create New</a></li>*@
                            </ul>
                        </li>
                        @*-----------------------------------------*@
                    </ul>
                    
                </div>
            </div>
        </div>
    </nav>
    <div class="container" style="margin-top:90px;">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
